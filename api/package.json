{
    "name": "groupomania_api",
    "version": "1.0.0",
    "description": "API for the Groupomania's social network",
    "main": "server.js",
    "type": "module",
    "scripts": {
        "test": "node --experimental-vm-modules node_modules/jest/bin/jest.js",
        "coverage": "node --experimental-vm-modules node_modules/jest/bin/jest.js --coverage",
        "start": "node -r dotenv/config ./src/server.js --env production",
        "dev": "nodemon -r dotenv/config ./src/server.js",
        "buildDoc": "redoc-cli build doc/groupomania_openapi.yaml -o doc/groupomania_doc.html --title \"Groupomania API\"",
        "watchDoc": "redocly preview-docs Groupomania --config=.redocly.yaml --port=8080"
    },
    "author": "Emeric Francisod",
    "license": "MIT",
    "devDependencies": {
        "@redocly/cli": "^1.0.0-beta.113",
        "@types/jest": "^29.2.1",
        "eslint": "^8.26.0",
        "jest": "^29.2.2",
        "nodemon": "^2.0.20",
        "redoc-cli": "^0.13.17"
    },
    "dependencies": {
        "@ladjs/graceful": "^3.2.1",
        "@ltd/j-toml": "^1.35.2",
        "@ucast/core": "^1.10.1",
        "@ucast/js": "^3.0.2",
        "@ucast/mongo": "^2.4.2",
        "bcrypt": "^5.1.0",
        "bree": "^9.1.3",
        "convict": "^6.2.3",
        "cors": "^2.8.5",
        "dotenv": "^16.0.3",
        "express": "^4.18.2",
        "express-validator": "^6.14.2",
        "helmet": "^6.0.0",
        "http-errors": "^2.0.0",
        "jsonwebtoken": "^9.0.0",
        "morgan": "^1.10.0",
        "multer": "^1.4.5-lts.1",
        "pg": "^8.8.0",
        "pg-hstore": "^2.3.4",
        "sequelize": "^6.25.6",
        "uuid": "^9.0.0",
        "winston": "^3.8.2",
        "winston-daily-rotate-file": "^4.7.1",
        "yargs-parser": "^21.1.1"
    }
}
